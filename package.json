{
  "name": "download-provisioning-profiles",
  "version": "4.0.0",
  "private": true,
  "description": "GitHub Action for downloading provisioning profiles",
  "main": "lib/main.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc --outDir lib",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "lint": "eslint",
    "package": "ncc build --license licenses.txt",
    "all": "yarn format && yarn lint && yarn build && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apple-Actions/download-provisioning-profiles.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Itty Bitty Apps Pty Ltd",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/io": "^1.0.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/luxon": "^1.21.0",
    "appstoreconnect": "github:orj/appstoreconnect#release/v0.3.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.22.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "prettier": "^1.19.1",
    "typescript": "^3.6.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
