{
  "name": "download-provisioning-profiles",
  "version": "4.0.0",
  "private": true,
  "description": "GitHub Action for downloading provisioning profiles",
  "main": "lib/main.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc --outDir lib",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "lint": "eslint",
    "package": "ncc build --license licenses.txt",
    "all": "yarn format && yarn lint && yarn build && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apple-Actions/download-provisioning-profiles.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Itty Bitty Apps Pty Ltd",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "~1.11.0",
    "@actions/io": "~1.1.0",
    "@types/jsonwebtoken": "~9.0.0",
    "@types/luxon": "~1.27.0",
    "appstoreconnect": "github:orj/appstoreconnect#release/v0.3.0",
    "jsonwebtoken": "~9.0.0",
    "luxon": "~1.28.0"
  },
  "devDependencies": {
    "@types/node": "~20.17.0",
    "@vercel/ncc": "~0.38.0",
    "eslint": "~9.25.0",
    "eslint-plugin-github": "~6.0.0",
    "knip": "~5.50.0",
    "prettier": "~3.5.0",
    "typescript": "~5.8.0"
  },
  "packageManager": "yarn@4.8.1"
}
